<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="font" format="string" />
    <attr name="uppercase" format="boolean" />
    <attr name="gradientStart" format="string" />
    <attr name="gradientEnd" format="string" />
    <attr name="underline" format="boolean" />
    <attr name="bold" format="boolean" />
    <attr name="roundedCorners" format="boolean" />
    <attr name="roundedCornersRadius" format="float" />
    <attr name="switchThumb" format="reference" />
    <attr name="switchTrack" format="reference" />
    <attr name="switchTextColors" format="reference" />
    <attr name="hintTextColor" format="color" />
    <attr name="hintUppercase" format="boolean" />
    <attr name="circleImageViewBorderStroke" format="dimension" />
    <attr name="circleImageViewBorderStrokeColor" format="color" />
    <attr name="colorTitle" format="color" />
    <attr name="colorSummary" format="color" />
    <attr name="textColor" format="color" />
    <attr name="textSize" format="dimension" />
    <attr name="titleTextSize" format="dimension" />
    <attr name="summaryTextSize" format="dimension" />
    <attr name="checkboxIcon" format="reference" />
    <attr name="blockCursor" format="boolean" />
    <attr name="inputRegex" format="enum">
        <enum name="date_ddmmyyyy" value="0" />
        <enum name="zip_code_pt" value="1" />
        <enum name="nif_pt" value="2" />
    </attr>
    <attr name="customRegex" format="string" />
    <attr name="square" format="boolean" />
    <attr name="preview1" format="reference" />
    <attr name="preview2" format="reference" />
    <attr name="preview3" format="reference" />
    <attr name="preview4" format="reference" />
    <attr name="text" format="string" />

    <declare-styleable name="TPExpandablePanel">
        <attr name="handle" format="reference" />
        <attr name="content" format="reference" />
        <attr name="collapsedHeight" format="dimension" />
        <attr name="animationDuration" format="integer" />
        <attr name="isOpened" format="boolean" />
    </declare-styleable>
    <declare-styleable name="TPFontableTextView">
        <attr name="font" />
        <attr name="uppercase" />
        <attr name="gradientStart" />
        <attr name="gradientEnd" />
        <attr name="underline" />
        <attr name="bold" />
        <attr name="minTextSize" format="dimension" />
        <attr name="precision" format="float" />
        <attr name="sizeToFit" format="boolean" />
    </declare-styleable>

    <declare-styleable name="TPFontableButton">
        <attr name="font" />
        <attr name="uppercase" />
    </declare-styleable>

    <declare-styleable name="TPFontableCheckBox">
        <attr name="font" />
        <attr name="uppercase" />
    </declare-styleable>

    <declare-styleable name="TPFontableRadioButton">
        <attr name="font" />
        <attr name="uppercase" />
    </declare-styleable>

    <declare-styleable name="TPRoundedImageView">
        <attr name="corner_radius" format="dimension" />
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
        <attr name="mutate_background" format="boolean" />
        <attr name="oval" format="boolean" />
        <attr name="android:scaleType" />
    </declare-styleable>

    <declare-styleable name="TPFontableEditText">
        <attr name="font" />
        <attr name="uppercase" />
        <attr name="underline" />
        <attr name="bold" />
        <attr name="hintTextColor" />
        <attr name="hintUppercase" />
        <attr name="blockCursor" />
        <attr name="inputRegex" />
        <attr name="customRegex" />
    </declare-styleable>

    <declare-styleable name="TPAnimatedViewPager">
        <attr name="vptyle">
            <enum name="standard" value="0" />
            <enum name="tablet" value="1" />
            <enum name="cubein" value="2" />
            <enum name="cubeout" value="3" />
            <enum name="flipvertical" value="4" />
            <enum name="fliphorizontal" value="5" />
            <enum name="stack" value="6" />
            <enum name="zoomin" value="7" />
            <enum name="zoomout" value="8" />
            <enum name="rotateup" value="9" />
            <enum name="rotatedown" value="10" />
            <enum name="accordion" value="11" />
        </attr>
        <attr name="fadeEnabled" format="boolean" />
        <attr name="outlineEnabled" format="boolean" />
        <attr name="outlineColor" format="color|reference" />
    </declare-styleable>

    <!-- Defines the custom XML attributes supported for a HorizontalListView -->
    <declare-styleable name="TPHorizontalListView">
        <attr name="dividerWidth" format="dimension" />
        <attr name="android:divider" />
        <attr name="android:requiresFadingEdge" />
        <attr name="android:fadingEdgeLength" />
    </declare-styleable>
    <declare-styleable name="TPCircularSeekBar">
        <attr name="progress" format="integer"></attr>
        <attr name="secondaryProgress" format="integer"></attr>
        <attr name="max" format="integer"></attr>
        <attr name="move_outside_circle" format="boolean"></attr>
        <attr name="maintain_equal_circle" format="boolean"></attr>
        <attr name="use_custom_radii" format="boolean"></attr>
        <attr name="circle_x_radius" format="float" />
        <attr name="circle_y_radius" format="float" />
        <attr name="circle_stroke_width" format="float" />
        <attr name="progress_stroke_width" format="float" />
        <attr name="secondary_progress_stroke_width" format="float" />
        <attr name="pointer_radius" format="float" />
        <attr name="pointer_halo_width" format="float" />
        <attr name="pointer_halo_border_width" format="float"></attr>
        <attr name="circle_color" format="string"></attr>
        <attr name="circle_progress_color" format="string"></attr>
        <attr name="circle_secondary_progress_color" format="string"></attr>
        <attr name="pointer_color" format="string"></attr>
        <attr name="pointer_halo_color" format="string"></attr>
        <attr name="pointer_alpha_ontouch" format="integer"></attr>
        <attr name="start_angle" format="float"></attr>
        <attr name="end_angle" format="float"></attr>
        <attr name="circle_fill" format="string"></attr>
        <attr name="pointer_drawable" format="integer" />
    </declare-styleable>

    <declare-styleable name="GifMoviewView">
        <attr name="gif" format="reference" />
        <attr name="paused" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CustomTheme">
        <attr name="gifMoviewViewStyle" format="reference" />
    </declare-styleable>

    <declare-styleable name="TPSquareImage">
        <attr name="snapToWidth" format="boolean" />
        <attr name="snapToHeight" format="boolean" />
    </declare-styleable>

    <declare-styleable name="TPFontablePreference">
        <attr name="font" />
    </declare-styleable>

    <declare-styleable name="TPFontableCheckboxPreference">
        <attr name="font" />
        <attr name="uppercase" />
        <attr name="titleTextSize" />
        <attr name="summaryTextSize" />
        <attr name="checkboxIcon" />
    </declare-styleable>

    <declare-styleable name="TPFontablePreferenceCategory">
        <attr name="font" />
        <attr name="uppercase" />
    </declare-styleable>

    <declare-styleable name="TPFontableSwitchPreference">
        <attr name="font" />
        <attr name="uppercase" />
        <attr name="switchThumb" />
        <attr name="switchTrack" />
        <attr name="switchTextColors" />
        <attr name="colorTitle" />
        <attr name="colorSummary" />
    </declare-styleable>

    <declare-styleable name="TPGroupPreviewLayout">
        <attr name="font" />
        <attr name="uppercase" />
        <attr name="textColor" />
        <attr name="textSize" />
        <attr name="square" />
        <attr name="preview1" />
        <attr name="preview2" />
        <attr name="preview3" />
        <attr name="preview4" />
        <attr name="text" />
    </declare-styleable>
</resources>